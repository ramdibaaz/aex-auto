#!/bin/bash
cd /dev/shm
git clone --filter=tree:0 https://github.com/AospExtended/platform_frameworks_base -b 11.x base
cd base
git remote add B1 https://github.com/ChintuOs/platform_frameworks_base
git fetch B1 11.x --depth=4
git cherry-pick 66a946ffba5ca3ede3226386fe34c510a8ed3579
git cherry-pick 8d1e4b98564a7ba65c2d8d357e83ba7d4d337940
git checkout -b auto
git push B1 -f
